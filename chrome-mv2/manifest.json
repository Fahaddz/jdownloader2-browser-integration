{
  "manifest_version": 2,
  "name": "JDownloader2 Browser Integration",
  "version": "2.1.2",
  "description": "Intercepts browser downloads and forwards them to JDownloader2",
  "homepage_url": "https://github.com/Lood2222/jdownloader2-browser-integration",
  "permissions": [
    "downloads",
    "storage",
    "webRequest",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon-128.png",
    "default_title": "Send downloads to JDownloader"
  },
  "icons": {
    "16": "icons/icon-128.png",
    "48": "icons/icon-128.png",
    "128": "icons/icon-128.png"
  }
}
